{
    "name": "thbe-bareos",
    "version": "0.4.4",
    "author": "thbe",
    "summary": "Install a Bareos enterprise class backup instance",
    "license": "GPL-3.0",
    "source": "https://github.com/thbe/puppet-bareos.git",
    "project_page": "https://github.com/thbe/puppet-bareos",
    "issues_url": "https://github.com/thbe/puppet-bareos/issues",
    "operatingsystem_support": [
